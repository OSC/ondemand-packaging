[ondemand-compute]
name=Open OnDemand Compute Repo
baseurl=https://yum.osc.edu/ondemand/@REPO@/compute/el$releasever/$basearch/
enabled=1
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute-SHA512

[ondemand-compute-source]
name=Open OnDemand Compute Repo - source
baseurl=https://yum.osc.edu/ondemand/@REPO@/compute/el$releasever/SRPMS/
enabled=0
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute-SHA512
