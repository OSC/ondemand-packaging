[ondemand-web]
name=Open OnDemand Web Repo
baseurl=https://yum.osc.edu/ondemand/@REPO@/web/el$releasever/$basearch/
enabled=1
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute-SHA512

[ondemand-web-source]
name=Open OnDemand Web Repo
baseurl=https://yum.osc.edu/ondemand/@REPO@/web/el$releasever/SRPMS/
enabled=0
gpgcheck=1
repo_gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute-SHA512
