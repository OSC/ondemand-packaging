[ood-compute]
name=Open OnDemand Compute Repo
baseurl=https://yum.osc.edu/ood/develop/compute/$DIST/$basearch/
enabled=1
gpgcheck=1
gpgkey=/etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute

[ood-compute-source]
name=Open OnDemand Compute Repo - source
baseurl=https://yum.osc.edu/ood/develop/compute/$DIST/SRPMS/
enabled=0
gpgcheck=1
gpgkey=/etc/pki/rpm-gpg/RPM-GPG-KEY-ondemand-compute
